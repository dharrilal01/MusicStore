@Master['_Layout']

@Section['Content']
<div id="promotion">
</div>

<h3><em>Fresh</em> off the grill</h3>

<ul id="album-list">
    @Each
    
        <li>
            <a href="/store/details/@Current.AlbumId">
                <img alt="@Current.Title" src="@Current.AlbumArtUrl" />
                <span>@Current.Title</span>
            </a>
        </li>
    @EndEach
</ul>
@EndSection
