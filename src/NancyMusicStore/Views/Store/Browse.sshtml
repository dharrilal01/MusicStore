@Master['_Layout']

@Section['Content'] 
<div class="genre">
    <h3><em>@ViewBag.Genre</em> Albums</h3>

    <ul id="album-list">
        @Each 
            <li>
                <a href="/store/details/@Current.AlbumId">
                    <img alt="@album.Title" src="@Current.AlbumArtUrl" />
                    <span>@Current.Title</span>
                </a>
            </li>
        @EndEach
    </ul>
</div>
@EndSection
